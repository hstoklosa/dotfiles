[
  /*
   * UI: Terminal
   */
  {
    "key": "ctrl+shift+oem_8",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "alt+k",
    "command": "workbench.action.terminal.resizePaneUp",
    "when": "terminal.active"
  },
  {
    "key": "alt+j",
    "command": "workbench.action.terminal.resizePaneDown",
    "when": "terminal.active"
  },
  {
    "key": "alt+m",
    "command": "workbench.action.toggleMaximizedPanel"
  },

  /*
   * UI: Sidebar
   */
  {
    "key": "ctrl+b",
    "command": "workbench.action.toggleActivityBarVisibility"
  },
  {
    "key": "ctrl+q",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpen"
  },
  {
    "command": "runCommands",
    "key": "alt+i",
    "args": {
      "commands": [
        "workbench.action.focusSideBar",
        "workbench.action.decreaseViewSize"
      ]
    }
  },
  {
    "command": "runCommands",
    "key": "alt+o",
    "args": {
      "commands": [
        "workbench.action.focusSideBar",
        "workbench.action.increaseViewSize"
      ]
    }
  },
  {
    "command": "runCommands",
    "key": "alt+y",
    "args": {
      "commands": [
        "workbench.action.focusAuxiliaryBar",
        "workbench.action.decreaseViewSize"
      ]
    }
  },
  {
    "command": "runCommands",
    "key": "alt+u",
    "args": {
      "commands": [
        "workbench.action.focusAuxiliaryBar",
        "workbench.action.increaseViewSize"
      ]
    }
  },
  // shift+alt+oem_8, alt+oem_8
  {
    "key": "ctrl+oem_8",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "alt+oem_8",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+`",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "alt+`",
    "command": "workbench.action.toggleSidebarVisibility"
  },

  /*
   * Extension: VIM
   */
  {
    "key": "ctrl+c",
    "command": "-extension.vim_ctrl+c",
    "when": "editorTextFocus && vim.active && vim.overrideCtrlC && vim.use<C-c> && !inDebugRepl"
  },
  {
    "key": "ctrl+b",
    "command": "-extension.vim_ctrl+b",
    "when": "editorTextFocus && vim.active && vim.use<C-b> && !inDebugRepl && vim.mode != 'Insert'"
  },
  {
    "key": "ctrl+w",
    "command": "-extension.vim_ctrl+w",
    "when": "editorTextFocus && vim.active && vim.use<C-w> && !inDebugRepl"
  },
  {
    "key": "ctrl+p",
    "command": "-extension.vim_ctrl+p",
    "when": "editorTextFocus && vim.active && vim.use<C-p> && !inDebugRepl || vim.active && vim.use<C-p> && !inDebugRepl && vim.mode == 'CommandlineInProgress' || vim.active && vim.use<C-p> && !inDebugRepl && vim.mode == 'SearchInProgressMode'"
  },
  //   {
  //     "key": "ctrl+t",
  //     "command": "-editor.action.indentLines",
  //     "when": "editorTextFocus && vim.active && vim.use<C-t> && !inDebugRepl"
  //   },
  //   {
  //     "key": "ctrl+t",
  //     "command": "-extension.vim_ctrl+t",
  //     "when": "editorTextFocus && vim.active && vim.use<C-t> && !inDebugRepl"
  //   },
  //   {
  //     "key": "ctrl+y",
  //     "command": "extension.vim_ctrl+y",
  //     "when": "editorTextFocus && vim.active && vim.use<C-y> && !inDebugRepl"
  //   },

  /*
   * EXTENSION: Github Copilot
   */
  {
    "key": "ctrl+alt+x",
    "command": "github.copilot.toggleCopilot"
  },
  {
    "key": "ctrl+i",
    "command": "inlineChat.start",
    "when": "inlineChatHasProvider && !editorReadonly"
  },
  {
    "key": "ctrl+alt+1",
    "command": "inlineChat.start",
    "when": "inlineChatHasProvider && !editorReadonly"
  },
  {
    "key": "ctrl+alt+2",
    "command": "github.copilot.chat.attachFile"
  },
  {
    "key": "ctrl+alt+3",
    "command": "github.copilot.chat.attachSelection"
  },

  /*
   * Extension: Turbo Console Log
   */
  {
    "key": "alt+oem_period",
    "command": "turboConsoleLog.displayLogMessage"
  },
  {
    "key": "ctrl+alt+l",
    "command": "-turboConsoleLog.displayLogMessage"
  },
  {
    "key": "ctrl+t",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "key": "ctrl+t",
    "command": "-extension.vim_ctrl+t",
    "when": "editorTextFocus && vim.active && vim.use<C-t> && !inDebugRepl"
  },
  {
    "key": "ctrl+shift+3",
    "command": "workbench.view.extension.claude-dev-ActivityBar"
  },
  {
    "key": "alt+k",
    "command": "-keybindings.editor.recordSearchKeys",
    "when": "inKeybindings && inKeybindingsSearch"
  },
  {
    "key": "alt+k",
    "command": "-editor.action.accessibilityHelpConfigureKeybindings",
    "when": "accessibilityHelpIsShown && accessibleViewHasUnassignedKeybindings"
  }
  //   {
  //     "key": "ctrl+b",
  //     "command": "-workbench.action.toggleSidebarVisibility"
  //   },
  //   {
  //     "key": "ctrl+alt+b",
  //     "command": "-workbench.action.toggleAuxiliaryBar"
  //   }
]
